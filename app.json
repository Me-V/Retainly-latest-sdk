{
  "expo": {
    "name": "Retainly",
    "slug": "expo-project",
    "owner": "hello_kitty14",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/companyLogo.png",
    "scheme": "education",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/companyLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.education.retainlyapp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/companyLogo.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-google-signin/google-signin",
      [
        "expo-splash-screen",
        {
          "image": "./assets/companyLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "621a65e5-fa0a-415e-bea7-07442f8365fc"
      }
    }
  }
}
